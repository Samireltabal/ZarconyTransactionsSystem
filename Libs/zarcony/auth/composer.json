{
    "name": "zarcony/auth",
    "description": "Authentication Package",
    "type": "library",
    "require": {
        "spatie/laravel-medialibrary": "^9.9.1",
        "spatie/laravel-permission": "^5.4",
        "laravel/passport": "^10.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Zarcony\\Auth\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Samir M. Eltabal",
            "email": "samir.m.eltabal@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Zarcony\\Auth\\ZarconyAuthServiceProvider"
            ],
            "aliases": {
                "ZarconyAuth": "Zarcony\\Auth\\ZarconyAuthFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
